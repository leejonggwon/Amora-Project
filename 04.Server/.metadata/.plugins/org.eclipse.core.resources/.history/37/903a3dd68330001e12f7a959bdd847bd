<%@ page language="java" contentType="text/html; charset=EUC-KR"
    pageEncoding="EUC-KR"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="EUC-KR">
<title>Insert title here</title>
</head>
<body>
	<%-- 반복문을 사용하여 10개짜리 방이 있는 테이블 만들기 --%>
	<table border ="1px solid black" >
	
	<!-- out.print방법 -->
	<tr>
		<% for(int i = 1; i <= 10; i++ ){
			out.print("<td>"+ i +"</td>");
		}
		%>
	</tr>
	
	<!-- 일일이 달아주는 기존방법 -->
	<tr>
		<% for(int i = 1; i <= 10; i++){ %>
			<td> <%= i %> </td>
		<% } %>
	</tr>
	
	</table>

</body>
</html>