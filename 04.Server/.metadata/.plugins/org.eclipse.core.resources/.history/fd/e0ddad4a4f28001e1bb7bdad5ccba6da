<body>
	<%-- 반복문을 사용하여 10개짜리 방이 있는 테이블 만들기 --%>
	<table border="1px solid black">
	
	
	<tr>	
		<% for(int i = 1; i<=10; i++){ 
	    
	       out.print("<td>" + i + "</td>");  // out객체를 활용하는 방법
	      
	    } %>
	</tr>
	
	
	<!--이전것 -->
	<tr>
		<% for(int i = 1; i<=10; i++){ %>  
	    
	      <td><%= i %></td>   <!-- *****이렇게 이어진 형태로 가능하다-->
	      
	    <% } %>
	</tr>
	
	
	</table>



</body>
